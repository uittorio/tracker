{
  "name": "tracker",
  "version": "1.0.0",
  "description": "tracker game",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config config/webpack.dev.config.js --inline --hot",
    "build": "rimraf build && webpack --config config/webpack/webpack.production.config.js",
    "test": "jest",
    "test-watch": "jest --watch",
    "update-snapShot": "jest --updateSnapshot"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/mocks/fileMock.js",
      "\\.(scss)$": "<rootDir>/src/test/mocks/styleMock.js"
    },
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/src/app/test/.*|\\.(spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "author": "Vittorio Guerriero",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "23.3.4",
    "@types/react": "16.4.16",
    "@types/react-dom": "16.0.8",
    "@types/react-redux": "6.0.11",
    "awesome-typescript-loader": "5.2.1",
    "copy-webpack-plugin": "4.5.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "node-sass": "4.9.3",
    "pretty-format": "23.6.0",
    "raw-loader": "0.5.1",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-redux": "6.0.0",
    "react-test-renderer": "16.5.2",
    "react.di": "2.0.0-beta.3",
    "redux": "4.0.1",
    "reflect-metadata": "0.1.12",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.4",
    "typescript": "3.1.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.1.4"
  }
}
